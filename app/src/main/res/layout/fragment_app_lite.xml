<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context=".appLite.AppLiteFragment">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/menu"
        android:name="com.example.app.appLite.conteudo.navBar.NavBarFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/conteudo"
        android:name="com.example.app.appLite.conteudo.inicial.InicialFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="-16dp"
        tools:layout_editor_absoluteY="266dp" />

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/dropDown"
        android:name="com.example.app.appLite.conteudo.navBar.DropdownMenuFragment"
        android:layout_width="194dp"
        android:layout_height="185dp"
        android:layout_marginTop="38dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>